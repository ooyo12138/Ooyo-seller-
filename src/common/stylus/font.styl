    url('../fonts/seller.ttf?ggam62') format('truetype'),
    url('../fonts/seller.woff?ggam62') format('woff'),
    url('../fonts/seller.svg?ggam62#seller') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="seller-"], [class*=" seller-"] {
